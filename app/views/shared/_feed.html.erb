<table id="goalsTable" class="table table-striped" cellspacing="0" width="100%">
	<thead>
		<tr>
			<th>Goal</th>
			<th>Votes</th>
			<th>Achieved</th>
		</tr>
	</thead>
	<tbody>
		<div class="checkRadioContainer">
			<% if @feed_items.any? %>
		  	<%= render @feed_items %>
		  	<%= will_paginate @feed_items %>
			<% end %>
		</div>
	</tbody>
</table>